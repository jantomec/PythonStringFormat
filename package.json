{
  "name": "pythonstringformat",
  "displayName": "PythonStringFormat",
  "description": "Convert between formatted strings in Python.",
  "version": "0.1.0",
  "publisher": "jantomec",
  "engines": {
    "vscode": "^1.98.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jantomec/PythonStringFormat.git"
  },
  "homepage": "https://github.com/jantomec/RunCellsWithTag#readme",
  "license": "MIT",
  "categories": [
    "Formatters"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "pythonstringformat.convertFstringToFormat",
        "title": "Convert f-string to .format(args)",
        "category": "PythonStringFormat"
      },
      {
        "command": "pythonstringformat.convertFstringToKeywordsFormat",
        "title": "Convert f-string to .format(keywords)",
        "category": "PythonStringFormat"
      },
      {
        "command": "pythonstringformat.convertFormatToFstring",
        "title": "Convert .format() to f-string",
        "category": "PythonStringFormat"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "extensionDependencies": [
    "ms-python.python"
  ],
  "dependencies": {
    "@types/vscode": "^1.98.0",
    "@vscode/python-extension": "^1.0.5"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.25.1",
    "eslint": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.2"
  }
}
